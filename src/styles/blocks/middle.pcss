.middle-section {
    padding: 140px 0;
    position: relative;
}
.middle-section__triangles-top {
    height: 100px;
    position: absolute;
    bottom: 100%;
    width: 100%;
}
.middle-section__triangles-bottom {
    height: 100px;
    position: absolute;
    top: 100%;
    width: 100%;
}

.section-title {
    font-weight: 700;
    font-size: 36px;
    color: $text-color;
}
